<h1>Stats</h1>

<div class="pure-g-r">
  <div class="pure-u-1-3">
    <span>Kanji count:</span> <strong>{{ kanjiCount }}</strong>
  </div>
  <div class="pure-u-1-3">
    <span>Recognizable:</span> <strong>{{ recognizable }}</strong>
  </div>
  <div class="pure-u-1-3">
    <span>Percentage:</span> <strong>{{ percentage }}%</strong>
  </div>
</div>

<h2>Unseen</h2>
{{#each list}}
  <a href="http://tangorin.com/kanji/{{ this }}" target="_blank"" lang="ja" class="grapheme kanji">{{ this }}</a>
{{/each}}
